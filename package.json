{
  "name": "",
  "version": "0.0.0",
  "description": "",
  "main": "./main.js",
  "scripts": {
    "dev-setup": "mkdir -p fonts && cp ./node_modules/pearson-elements/dist/fonts/* ./fonts && cp ./node_modules/pearson-elements/dist/css/elements.css ./demo",
    "dev": "npm run dev-setup && webpack-dev-server --port 8081 --devtool eval --hot --progress --colors",
    "pretest": "npm run lint",
    "test": "jest",
    "pretest-cov": "npm run lint",
    "test-cov": "jest --coverage",
    "build": "webpack -p",
    "coveralls": "coveralls < coverage/lcov.info",
    "lint": "eslint src/js/**",
    "lint-all": "eslint src/js/** __tests__/** demo/*.js",
    "gen-changelog": "node ./npm_scripts/gen-changelog.js",
    "version": "npm run gen-changelog && git add CHANGELOG.md",
    "ci": "npm run test-cov && npm run build",
    "release": "node ./npm_scripts/release.js",
    "verify": "node ./npm_scripts/verify.js"
  },
  "jest": {
    "testRunner": "./node_modules/jest-cli/src/testRunners/jasmine/jasmine2",
    "scriptPreprocessor": "./node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pearson-Higher-Ed/"
  },
  "dependencies": {
    "react-intl": "~2.0.1"
  },
  "devDependencies": {
    "babel-core": "~6.3.17",
    "babel-eslint": "~4.1.6",
    "babel-jest": "~6.0.1",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.5.0",
    "coveralls": "~2.11.9",
    "css-loader": "~0.23.0",
    "enzyme": "~2.2.0",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.1.1",
    "eslint-plugin-react": "~3.16.1",
    "file-loader": "~0.8.5",
    "jest-cli": "~0.9.0",
    "json-loader": "~0.5.4",
    "node-sass": "^3.4.2",
    "pearson-elements": "^0.3.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "sass-loader": "^3.2.0",
    "semver": "~5.1.0",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0"
  },
  "keywords": [
    "pearson-components"
  ]
}
