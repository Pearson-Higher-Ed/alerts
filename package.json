{
  "name": "@pearson-components/alerts",
  "version": "1.2.1",
  "description": "Component to render alerts",
  "main": "./build/dist.alerts.js",
  "scripts": {
    "dev-setup": "node ./npm_scripts/dev-setup.js",
    "dev": "node ./npm_scripts/dev.js",
    "start": "npm run dev",
    "pretest": "node ./npm_scripts/pretest.js",
    "test": "jest --coverage",
    "build": "node ./npm_scripts/build.js",
    "lint": "node ./npm_scripts/lint.js",
    "gen-changelog": "node ./npm_scripts/gen-changelog.js",
    "version": "node ./npm_scripts/version.js",
    "ci": "node ./npm_scripts/ci.js",
    "release": "node ./npm_scripts/release.js",
    "verify": "node ./npm_scripts/verify.js",
    "copy-utils": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/cross-platform.js",
    "postpublish": "npm access public && echo 'Package scope set to public!'"
  },
  "author": "Pearson Design Accelerator",
  "license": "PEARSON PROPRIETARY AND CONFIDENTIAL INFORMATION SUBJECT TO NDA",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pearson-Higher-Ed/alerts"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "**/src/js/AlertList.js",
      "**/src/js/Alert.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|demo|fonts|npm_scripts|translations|coverage)/"
    ],
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "react-addons-css-transition-group": "^15.4.2"
  },
  "devDependencies": {
    "@pearson-components/npm-scripts": "^0.3.7",
    "babel-core": "~6.3.17",
    "babel-eslint": "~4.1.6",
    "babel-jest": "^18.0.0",
    "babel-loader": "~6.2.0",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.7.2",
    "colors": "~1.1.2",
    "conventional-changelog": "~1.1.0",
    "css-loader": "~0.23.0",
    "enzyme": "^2.7.0",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.1.1",
    "eslint-plugin-react": "~3.16.1",
    "expect": "~1.18.0",
    "expect-jsx": "~2.5.1",
    "file-loader": "~0.8.5",
    "fs-extra": "^1.0.0",
    "ignore-styles": "~5.0.1",
    "intl": "~1.1.0",
    "jest": "^18.1.0",
    "jsdom": "^8.5.0",
    "json-loader": "~0.5.4",
    "mocha": "~2.4.5",
    "node-sass": "~3.7.0",
    "pearson-elements": "^1.14.0",
    "prop-types": "^15.5.10",
    "react": "15.6.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "15.6.0",
    "react-transition-group": "^1.2.0",
    "sass-loader": "~3.2.0",
    "semver": "~5.1.0",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0"
  },
  "keywords": [
    "pearson-components"
  ]
}
